                        <option value="毛南族">毛南族</option>
                        <option value="撒拉族">撒拉族</option>
                    </select>
                </div>
            </div>
            <div class="form-group row">
                <label for="idnumber" class="col-sm-2 form-control-label">身份证号</label>
                <div class="col-sm-10">
                    <input id="idnumber" type="text" class="form-control" name="idnumber">
                </div>
            </div>
            <div class="form-group row">
                <label for="politicalstatus" class="col-sm-2 form-control-label">政治面貌</label>
