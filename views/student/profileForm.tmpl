    <body>
    <div class="container-fluid">
        <form id="profileForm" method="POST">
            <div class="form-grop row">
                <label for="id" class="col-sm-2 form-control-label">学号</label>
                <div class="col-sm-10">
                    <input id="id" type="text" class="form-control" name="id" readonly value="{% .ID %}">
                </div>
            </div>
            <div class="form-grop row">
                <label for="name" class="col-sm-2 form-control-label">姓名</label>
                <div class="col-sm-10">
                    <input id="name" type="text" class="form-control" name="name">
                </div>
            </div>
            <div class="form-group row">
                <label for="sex" class="col-sm-2 form-control-label">性别</label>
                <div class="col-sm-10">
                    <select id="sex" class="form-control" name="sex">
                        <option value="女" selected>女</option>
                        <option value="男">男</option>
                    </select>
                </div>
            </div>
            <div class="form-grop row">
                <label for="birthdate" class="col-sm-2 form-control-label">出生日期</label>
                <div class="col-sm-10">
                    <input id="birthdate" type="date" class="form-control" name="birthdate">
                </div>
            </div>
